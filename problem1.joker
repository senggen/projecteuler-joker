(defn sum_of_multiples_3_or_5_below 
  [max]
  (let [nums (range 1 max)
        multiples (filter #(or (= (mod %1 3) 0) (= (mod %1 5) 0)) nums)
        sum (reduce + multiples)]
    sum))

(println "sum of multiples 3 or 5 below 1000 is" (sum_of_multiples_3_or_5_below 1000))
;; => sumb of multiples 3 or 5 below 1000 is 233168
